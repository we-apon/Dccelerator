{
  "version": "0.0.7-beta-004",
  "title": "Dccelerator.DataAccess.Common",
  "description": "Contains interfaces and base implementation of Dccelerator.DataAccess ORM.",
  "summary": "Base interfaces of Dccelerator.DataAccess ORM.",
  "authors": [ "Kirill Dubinkin" ],
  "tags": [ "ORM" ],
  "projectUrl": "https://github.com/we-apon/Dccelerator",
  "licenseUrl": "https://github.com/we-apon/Dccelerator/blob/master/LICENSE",


  "configurations": {
    "Debug": { "compilationOptions": { "define": [ "DEBUG" ] } },
    "Release": { "compilationOptions": { "optimize": true } }
  },



  "frameworks": {

    "net40": {
      "compilationOptions": { "define": [ "NET40", "JETBRAINS_ANNOTATIONS" ] }
    },



    "net45": {
      "compilationOptions": { "define": [ "NET45", "JETBRAINS_ANNOTATIONS" ] }
    },



    "dnx45": {
      "compilationOptions": { "define": [ "NET45", "JETBRAINS_ANNOTATIONS" ] }
    }


  },



  "dependencies": {
    "Dccelerator.Core": ""
  },


  "scripts": {
    "postbuild": [
      "if not exist \"%project:BuildOutputDir%\\..\\Last\\net40\" mkdir %project:BuildOutputDir%\\..\\Last\\net40",
      "if not exist \"%project:BuildOutputDir%\\..\\Last\\net45\" mkdir %project:BuildOutputDir%\\..\\Last\\net45",
      "if not exist \"%project:BuildOutputDir%\\..\\Last\\dnx45\" mkdir %project:BuildOutputDir%\\..\\Last\\dnx45",
      "copy /V /Y %project:BuildOutputDir%\\Debug\\net40\\* %project:BuildOutputDir%\\..\\Last\\net40 /B",
      "copy /V /Y %project:BuildOutputDir%\\Debug\\net45\\* %project:BuildOutputDir%\\..\\Last\\net45 /B",
      "copy /V /Y %project:BuildOutputDir%\\Debug\\dnx45\\* %project:BuildOutputDir%\\..\\Last\\dnx45 /B"
    ]
  }

}
