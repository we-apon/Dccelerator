{
  "version": "0.0.12-*",
  "title": "Berkeley Db adapter for Dccelerator.DataAccess ORM",
  "description": "Berkeley Db adapter for Dccelerator.DataAccess ORM",
  "authors": [ "Kirill Dubinkin" ],
  "packOptions": {
    "tags": [ "ORM BerkeleyDb" ],
    "projectUrl": "https://github.com/we-apon/Dccelerator",
    "licenseUrl": "https://github.com/we-apon/Dccelerator/blob/master/LICENSE"
  },


  "configurations": {
    "Debug": {
      "buildOptions": {
        "define": [ "DEBUG", "TRACE" ],
        "xmlDoc": true
      }
    },
    "Release": {
      "buildOptions": {
        "define": [ "TRACE" ],
        "optimize": true,
        "xmlDoc": true
      }
    }
  },

  "frameworks": {
    "net40": {
      "buildOptions": { "define": [ "NET40", "JETBRAINS_ANNOTATIONS" ] }
    }

  },

  "dependencies": {
    "Dccelerator.DataAccess.Common": "0.0.12-*",
    "Berkeley.DB": "6.1.26"
  },


  "scripts": {
    "postcompile": [
      "../../../NugetPack.bat %compile:Configuration% Release ../../../publish"
    ]
  }
}
