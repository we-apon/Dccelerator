{
  "version": "0.0.12-*",
  "title": "Dccelerator.DataAccess.Adapters.MsSql",
  "description": "Contains Microsoft SQL Server adapter for Dccelerator.DataAccess ORM.",
  "authors": [ "Kirill Dubinkin" ],
  "packOptions": {
    "summary": "Microsoft SQL Server adapter for Dccelerator.DataAccess ORM.",
    "tags": [ "ORM ADO.NET SQLClient MSSQL SQL" ],
    "projectUrl": "https://github.com/we-apon/Dccelerator",
    "licenseUrl": "https://github.com/we-apon/Dccelerator/blob/master/LICENSE"
  },

  "configurations": {
    "Debug": { "buildOptions": { "define": [ "DEBUG" ] } },
    "Release": {
      "buildOptions": {
        "optimize": true,
        "xmlDoc": true
      }
    }
  },

  "frameworks": {
    "net40": {
      "buildOptions": { "define": [ "NET40", "JETBRAINS_ANNOTATIONS" ] },

      "frameworkAssemblies": {
        "System.Xml": "4.0.0.0",
        "System.Transactions": "4.0.0.0"
      }
    },

    "net45": {
      "buildOptions": { "define": [ "NET45", "JETBRAINS_ANNOTATIONS" ] },

      "frameworkAssemblies": {
        "System.Xml": "4.0.0.0",
        "System.Transactions": "4.0.0.0"
      }
    }

  },
  "dependencies": {
    "Dccelerator.DataAccess.AdoNet": "0.0.12-*"
  },

}
