{
  "version": "0.0.11-beta-008",
  "title": "Dccelerator's core utilities",
  "description": "Core utitities of Dccelerator framework. \nIt contains some nice stuff like RUtils, SmartConvert and many usefull reflection extensions, some of that works through runtime delegate generation.",
  "summary": "Core utitities of Dccelerator framework.",
  "authors": [ "Kirill Dubinkin" ],
  "copyright": "Copyright © Kirill Dubinkin 2015-2016",
  "tags": [ "Reflection Convertion Extensions" ],
  "projectUrl": "https://github.com/we-apon/Dccelerator",
  "licenseUrl": "https://github.com/we-apon/Dccelerator/blob/master/LICENSE",


  "configurations": {
    "Debug": { "compilationOptions": { "define": [ "DEBUG" ] } },
    "Release": { "compilationOptions": { "optimize": true } }
  },



  "frameworks": {

    "net40": {
      "compilationOptions": { "define": [ "NET40", "JETBRAINS_ANNOTATIONS" ] },
      "dependencies": {
        "JetBrains.Annotations": "10.0.0"
      }
    },



    "net45": {
      "compilationOptions": { "define": [ "NET45", "JETBRAINS_ANNOTATIONS" ] },
      "dependencies": {
        "JetBrains.Annotations": "10.0.0"
      }
    },



    "dnx45": {
      "compilationOptions": { "define": [ "NET45", "JETBRAINS_ANNOTATIONS" ] },
      "dependencies": {
        "JetBrains.Annotations": "10.0.0"
      }
    },


    "dotnet": {
      "compilationOptions": { "define": [ "DOTNET", "JETBRAINS_ANNOTATIONS" ] },
      "dependencies": {
        "System.Runtime": "4.0.20",
        "Microsoft.CSharp": "4.0.0",
        "System.Collections": "4.0.10",
        "System.Collections.Concurrent": "4.0.10",
        "System.Reflection": "4.0.10",
        "System.Linq": "4.0.0",
        "System.Diagnostics.TraceSource": "4.0.0-beta-23409"
      }
    }

  },

  "dependencies": {
  },


  "scripts": {
    "postbuild": [
      "if not exist \"%project:BuildOutputDir%\\..\\Last\\net40\" mkdir %project:BuildOutputDir%\\..\\Last\\net40",
      "if not exist \"%project:BuildOutputDir%\\..\\Last\\net45\" mkdir %project:BuildOutputDir%\\..\\Last\\net45",
      "if not exist \"%project:BuildOutputDir%\\..\\Last\\dnx45\" mkdir %project:BuildOutputDir%\\..\\Last\\dnx45",
      "if not exist \"%project:BuildOutputDir%\\..\\Last\\dotnet\" mkdir %project:BuildOutputDir%\\..\\Last\\dotnet",
      "copy /V /Y %project:BuildOutputDir%\\Debug\\net40\\* %project:BuildOutputDir%\\..\\Last\\net40 /B",
      "copy /V /Y %project:BuildOutputDir%\\Debug\\net45\\* %project:BuildOutputDir%\\..\\Last\\net45 /B",
      "copy /V /Y %project:BuildOutputDir%\\Debug\\dnx45\\* %project:BuildOutputDir%\\..\\Last\\dnx45 /B",
      "copy /V /Y %project:BuildOutputDir%\\Debug\\dotnet\\* %project:BuildOutputDir%\\..\\Last\\dotnet /B"
    ]
  }
}
